{
    "name": "pinknoisebabies/data-fixtures",
    "type": "library",
    "description": "Data Fixtures for all Doctrine Object Managers",
    "keywords": ["database"],
    "homepage": "https://github.com/pinknoisebabies",
    "license": "MIT",
    "authors": [
        {"name": "pinknoisebabies", "email": "github@kick-their-ass.co.uk"}
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/common": "~2.2"
    },
    "require-dev": {
        "doctrine/orm": "~2.4"
    },
    "conflict": {
        "doctrine/orm": "< 2.4"
    },
    "suggest": {
        "doctrine/orm": "For loading ORM fixtures",
        "doctrine/mongodb-odm": "For loading MongoDB ODM fixtures",
        "doctrine/phpcr-odm": "For loading PHPCR ODM fixtures"
    },
    "autoload": {
        "psr-0": { "Doctrine\\Common\\DataFixtures": "lib/" }
    },
    "autoload-dev": {
        "psr-0": { "Doctrine\\Tests": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
